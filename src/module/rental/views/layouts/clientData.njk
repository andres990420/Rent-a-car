<div class="columns is-hidden">
    <div class="column is-one-third">
        <label class="label">Tipo de documento</label>
        <div class="field">
            <div class="control box is-flex">
                <span class="icon is-left"><i class="fas fa-passport"></i></span>
                <p id="input-document-type"></p>
            </div>
        </div>
    </div>
    <div class="column is-one-third">
        <label class="label">Numero de documento</label>
        <div class="field">
            <div class="control box is-flex">
                <span class="icon is-left"><i class="fas fa-hashtag"></i></span>
                <p id="input-document-number"></p>
            </div>
        </div>
    </div>
    <div class="column is-one-third">
        <label class="label">Nacionalidad</label>
        <div class="field">
            <div class="control box is-flex">
                <span class="icon is-left"><i class="fas fa-globe"></i></span>
                <p id="input-nationality"></p>
            </div>
        </div>
    </div>
</div>
<div class="columns is-hidden">
    <div class="column is-half">
        <label class="label">Email</label>
        <div class="field">
            <div class="control box is-flex">
                <span class="icon"><i class="fas fa-envelope"></i></span>
                <p id="input-email"></p>
            </div>
        </div>
    </div>
    <div class="column is-half">
        <label class="label">Numero de telefono</label>
        <div class="field">
            <div class="control box is-flex">
                <span class="icon"><i class="fas fa-phone"></i></span>
                <p id="input-phone-number"></p>
            </div>
        </div>
    </div>
</div>